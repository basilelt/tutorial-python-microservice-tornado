apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: {{ CERTIFICATE_AUTHORITY_DATA }}
    server: https://{{ CLUSTER_URL }}:6443
  name: {{ CLUSTER_URL }}
contexts:
- context:
    cluster: {{ CLUSTER_URL }}
    user: kubernetes-admin-{{ CLUSTER_URL }}
  name: kubernetes-admin-{{ CLUSTER_URL }}@{{ CLUSTER_URL }}
current-context: kubernetes-admin-{{ CLUSTER_URL }}@{{ CLUSTER_URL }}
kind: Config
preferences: {}
users:
- name: kubernetes-admin-{{ CLUSTER_URL }}
  user:
    client-certificate-data: {{ CLIENT_CERTIFICATE_DATA }}
    client-key-data: {{ CLIENT_KEY_DATA }}
